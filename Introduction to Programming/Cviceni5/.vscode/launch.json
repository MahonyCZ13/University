    "version": "0.2.0",
    "configurations": [
        {
            "name": "Sandbox",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/sandbox/bin/Debug/netcoreapp3.1/sandbox.dll",
            "args": [],
            "cwd": "${workspaceFolder}/sandbox",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Priklad3",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-priklad3",
            "program": "${workspaceFolder}/priklad3/bin/Debug/netcoreapp3.1/priklad3.dll",
            "args": [],
            "cwd": "${workspaceFolder}/priklad3",
            "console":"integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}